{
  "name": "grasis-web",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "start:dev": "next dev . -p 3001",
    "build": "yarn type-check && next build .",
    "start:prod": "next start -p $PORT",
    "install:dependencies": "yarn install",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && npm install && yarn dev",
    "prune:dependencies": "npm prune --production",
    "type-check": "tsc",
    "type-check:dev": "tsc -w"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.7.3",
    "antd-img-crop": "^3.13.1",
    "axios": "^0.21.0",
    "http-status-codes": "^2.1.4",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.0.0",
    "moment": "^2.29.1",
    "next": "^10.0.0",
    "next-compose-plugins": "2.2.0",
    "next-cookies": "^2.0.3",
    "next-images": "1.6.2",
    "nprogress": "^0.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "^0.3.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^14.14.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "typescript": "^4.0.5",
    "webpack": "^5.3.2"
  }
}
